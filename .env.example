# Tududi API Configuration
TUDUDI_API_URL=http://localhost:3000

# Authentication: Use EITHER API key OR email/password
# Option 1: API Key (for newer Tududi versions)
TUDUDI_API_KEY=your-api-key-here

# Option 2: Email/Password (for older Tududi versions without API token support)
# TUDUDI_EMAIL=your-email@example.com
# TUDUDI_PASSWORD=your-password

# Logging
LOG_LEVEL=info

